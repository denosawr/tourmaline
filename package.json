{
    "name": "tourmaline",
    "version": "1.0.0",
    "main": "main.js",
    "repository": "https://github.com/denosawr/tourmaline",
    "author": "denosawr <denosawr@vivaldi.net>",
    "private": true,
    "scripts": {
        "start": "electron .",
        "rebuild": "./node_modules/.bin/electron-rebuild",
        "pretest": "prettier --write ./**/*.js",
        "package": "electron-packager . tourmaline --ignore='app/tourmaline-helper|tourmaline-helper'  --platform=darwin --overwrite && cp app/tourmaline-helper/tourmaline-helper-app tourmaline-darwin-x64/tourmaline.app/Contents/Frameworks/tourmaline-helper-app",
        "rundev": "open ./tourmaline-darwin-x64/tourmaline.app --args --dev",
        "kill": "killall tourmaline"
    },
    "dependencies": {
        "cpu-stat": "^2.0.1",
        "diskusage": "^1.0.0",
        "express": "^4.16.4",
        "ffi": "^2.3.0",
        "global": "^4.3.2",
        "ps-list": "^6.1.0",
        "ref": "^1.3.5",
        "socket.io": "^2.2.0",
        "strftime": "^0.10.0",
        "wallpaper": "^4.2.0",
        "zeromq": "^5.1.0"
    },
    "devDependencies": {
        "electron": "^4.0.4",
        "electron-packager": "^13.0.1",
        "electron-rebuild": "^1.8.2",
        "nan": "2.11.1",
        "prettier": "1.16.1"
    }
}
